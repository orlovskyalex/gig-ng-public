{
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "rxjs-tslint-rules",
    "tslint-etc"
  ],
  "rules": {
    "angular-whitespace": [
      true,
      "check-interpolation",
      "check-pipe",
      "check-semicolon"
    ],
    "array-bracket-spacing": [
      true,
      "never"
    ],
    "arrow-parens": true,
    "arrow-return-shorthand": true,
    "await-promise": true,
    "ban-comma-operator": true,
    "ban-ts-ignore": true,
    "brace-style": [
      true,
      "1tbs"
    ],
    "callable-types": true,
    "class-name": true,
    "component-class-suffix": true,
    "component-max-inline-declarations": false,
    "component-selector": [
      true,
      "app",
      "element",
      "kebab-case"
    ],
    "contextual-lifecycle": true,
    "cyclomatic-complexity": [
      true,
      20
    ],
    "deprecation": {
      "severity": "warning"
    },
    "directive-class-suffix": true,
    "directive-selector": [
      true,
      "app",
      "attribute",
      "camelCase"
    ],
    "encoding": true,
    "eofline": true,
    "file-name-casing": [
      true,
      "kebab-case"
    ],
    "import-blacklist": {
      "options": [
        "@angular/material",
        "rxjs/Rx"
      ]
    },
    "import-destructuring-spacing": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": {
      "options": "never-prefix"
    },
    "linebreak-style": {
      "options": "LF"
    },
    "max-line-length": {
      "options": {
        "ignore-pattern": "^import [^,]+ from|class [a-zA-Z]+|loadChildren(?!.*?then)|^} from",
        "limit": 120
      }
    },
    "member-access": {
      "options": "no-public"
    },
    "member-ordering": {
      "options": {
        "order": [
          "public-static-field",
          "protected-static-field",
          "private-static-field",
          "public-static-method",
          "protected-static-method",
          "private-static-method",
          "public-instance-field",
          "protected-instance-field",
          "private-instance-field",
          "constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    },
    "newline-before-return": true,
    "newline-per-chained-call": false,
    "no-angle-bracket-type-assertion": true,
    "no-any": true,
    "no-assign-mutated-array": true,
    "no-async-without-await": true,
    "no-attribute-decorator": true,
    "no-boolean-literal-compare": true,
    "no-conflicting-lifecycle": true,
    "no-const-enum": true,
    "no-constant-condition": true,
    "no-default-export": true,
    "no-duplicate-case": true,
    "no-duplicate-imports": true,
    "no-duplicate-switch-case": true,
    "no-dynamic-delete": true,
    "no-empty": {
      "options": "allow-empty-catch"
    },
    "no-empty-character-class": true,
    "no-ex-assign": true,
    "no-extra-boolean-cast": true,
    "no-floating-promises": true,
    "no-for-in-array": true,
    "no-host-metadata-property": true,
    "no-implicit-dependencies": [
      true,
      "dev",
      [
        "@app",
        "@env"
      ]
    ],
    "no-import-side-effect": {
      "options": {
        "ignore-module": "zone.js|^\\./"
      }
    },
    "no-inferrable-types": {
      "options": "ignore-params"
    },
    "no-inferred-empty-object-type": true,
    "no-inner-declarations": true,
    "no-input-rename": true,
    "no-inputs-metadata-property": true,
    "no-invalid-template-strings": true,
    "no-magic-numbers": false,
    "no-multi-spaces": true,
    "no-non-null-assertion": true,
    "no-object-literal-type-assertion": true,
    "no-output-native": true,
    "no-output-on-prefix": true,
    "no-output-rename": true,
    "no-outputs-metadata-property": true,
    "no-parameter-reassignment": true,
    "no-pipe-impure": true,
    "no-promise-as-boolean": true,
    "no-queries-metadata-property": true,
    "no-redundant-jsdoc": true,
    "no-reference": true,
    "no-reference-import": true,
    "no-regex-spaces": true,
    "no-return-await": true,
    "no-require-imports": true,
    "no-shadowed-variable": {
      "options": {
        "temporalDeadZone": false
      }
    },
    "no-sparse-arrays": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-this-assignment": true,
    "no-trailing-whitespace": true,
    "no-unexpected-multiline": true,
    "no-unnecessary-callback-wrapper": true,
    "no-unnecessary-initializer": true,
    "no-unnecessary-type-assertion": true,
    "no-unsafe-any": false,
    "no-unused-css": true,
    "no-unused-declaration": true,
    "no-unused-expression": {
      "options": "allow-fast-null-checks"
    },
    "no-unused-variable": false,
    "no-use-before-declare": false,
    "no-void-expression": true,
    "number-literal-format": true,
    "object-curly-spacing": [
      true,
      "always",
      {
        "arraysInObjects": true,
        "objectsInObjects": true
      }
    ],
    "object-literal-key-quotes": {
      "options": "as-needed"
    },
    "object-literal-shorthand": true,
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "prefer-conditional-expression": {
      "options": "check-else-if"
    },
    "prefer-function-over-method": false,
    "prefer-inline-decorator": [
      true,
      "HostListener",
      "Input"
    ],
    "prefer-method-signature": true,
    "prefer-object-spread": true,
    "prefer-on-push-component-change-detection": true,
    "prefer-output-readonly": true,
    "prefer-readonly": false,
    "prefer-template": {
      "options": "allow-single-concat"
    },
    "prefer-while": true,
    "promise-function-async": true,
    "quotemark": {
      "options": [
        "avoid-escape",
        "avoid-template",
        "single"
      ]
    },
    "radix": true,
    "restrict-plus-operands": true,
    "return-undefined": true,
    "rxjs-ban-observables": true,
    "rxjs-ban-operators": true,
    "rxjs-finnish": {
      "options": [
        {
          "functions": false,
          "methods": false,
          "parameters": true,
          "properties": true,
          "variables": true
        },
        {
          "types": {
            "^EventEmitter$": false
          }
        }
      ]
    },
    "rxjs-no-async-subscribe": true,
    "rxjs-no-compat": true,
    "rxjs-no-create": true,
    "rxjs-no-exposed-subjects": true,
    "rxjs-no-ignored-observable": true,
    "rxjs-no-ignored-replay-buffer": true,
    "rxjs-no-internal": true,
    "rxjs-no-nested-subscribe": true,
    "rxjs-no-subject-unsubscribe": true,
    "rxjs-no-subject-value": true,
    "rxjs-no-unsafe-catch": true,
    "rxjs-no-unsafe-first": true,
    "rxjs-no-unsafe-switchmap": true,
    "rxjs-no-unsafe-takeuntil": true,
    "rxjs-throw-error": {
      "severity": "warning"
    },
    "semicolon": {
      "options": [
        "always",
        "ignore-bound-class-methods"
      ]
    },
    "space-before-function-paren": {
      "options": {
        "anonymous": "always",
        "asyncArrow": "always",
        "constructor": "never",
        "method": "never",
        "named": "never"
      }
    },
    "space-in-parens": true,
    "strict-boolean-expressions": false,
    "strict-type-predicates": true,
    "switch-default": true,
    "template-banana-in-box": true,
    "template-conditional-complexity": [
      true,
      3
    ],
    "template-no-any": true,
    "template-no-call-expression": false,
    "template-no-negated-async": true,
    "template-use-track-by-function": false,
    "ter-func-call-spacing": true,
    "ter-newline-after-var": true,
    "ter-no-mixed-spaces-and-tabs": true,
    "ter-no-proto": true,
    "ter-no-self-compare": true,
    "ter-padded-blocks": {
      "options": {
        "blocks": "never",
        "classes": "always",
        "switches": "never"
      }
    },
    "ter-prefer-arrow-callback": true,
    "trailing-comma": {
      "options": {
        "esSpecCompliant": true,
        "multiline": "always",
        "singleline": "never"
      }
    },
    "typedef": {
      "options": [
        "call-signature",
        "parameter",
        "property-declaration"
      ]
    },
    "typedef-whitespace": {
      "options": [
        {
          "call-signature": "nospace",
          "index-signature": "nospace",
          "parameter": "nospace",
          "property-declaration": "nospace",
          "variable-declaration": "nospace"
        },
        {
          "call-signature": "onespace",
          "index-signature": "onespace",
          "parameter": "onespace",
          "property-declaration": "onespace",
          "variable-declaration": "onespace"
        }
      ]
    },
    "unified-signatures": true,
    "unnecessary-constructor": {
      "options": "check-super-calls"
    },
    "unnecessary-else": {
      "options": "allow-else-if"
    },
    "use-component-selector": true,
    "use-component-view-encapsulation": true,
    "use-default-type-parameter": true,
    "use-lifecycle-interface": true,
    "use-pipe-decorator": true,
    "use-pipe-transform-interface": true,
    "valid-typeof": true,
    "variable-name": {
      "options": [
        "allow-pascal-case",
        "ban-keywords",
        "check-format"
      ]
    },
    "whitespace": {
      "options": [
        "check-branch",
        "check-decl",
        "check-module",
        "check-operator",
        "check-preblock",
        "check-rest-spread",
        "check-separator",
        "check-type",
        "check-type-operator"
      ]
    }
  }
}
