{
  "name": "gig-ng-public",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "yarn check-node-version && yarn ngcc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "yarn tslint:all && yarn sass-lint",
    "tslint": "tslint -p . -c ./tslint.json",
    "tslint:all": "yarn tslint './src/**/*.ts'",
    "sass-lint": "sass-lint",
    "check-node-version": "node check_node_version.js",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/forms": "~9.0.1",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@datorama/akita": "^4.19.2",
    "@ngneat/until-destroy": "^7.1.1",
    "bootstrap": "^4.4.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@datorama/akita-cli": "^6.1.0",
    "@types/node": "^12.11.1",
    "angular-tslint-rules": "^1.20.3",
    "codelyzer": "^5.2.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "rxjs-tslint-rules": "^4.28.3",
    "sass-lint": "^1.13.1",
    "semver": "^7.1.3",
    "ts-node": "~8.3.0",
    "tslint": "^5.20",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-etc": "^1.10.0",
    "typescript": "~3.7.5"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=10.16"
  },
  "akitaCli": {
    "customFolderName": "true",
    "template": "angular",
    "basePath": "./src/app/"
  }
}
